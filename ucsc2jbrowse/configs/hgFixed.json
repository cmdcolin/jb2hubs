{
  "plugins": [
    {
      "name": "MafViewer",
      "url": "https://unpkg.com/jbrowse-plugin-mafviewer/dist/jbrowse-plugin-mafviewer.umd.production.min.js"
    },
    {
      "name": "Hubs",
      "url": "https://unpkg.com/@cmdcolin/jbrowse-plugin-hubs/dist/jbrowse-plugin-hubs.umd.production.min.js"
    },
    {
      "name": "Protein3d",
      "url": "https://unpkg.com/jbrowse-plugin-protein3d/dist/jbrowse-plugin-protein3d.umd.production.min.js"
    },
    {
      "name": "MsaView",
      "url": "https://unpkg.com/jbrowse-plugin-msaview/dist/jbrowse-plugin-msaview.umd.production.min.js"
    }
  ],
  "assemblies": [
    {
      "name": "hgFixed",
      "displayName": "undefined (hgFixed)",
      "sequence": {
        "type": "ReferenceSequenceTrack",
        "trackId": "hgFixed-refseq",
        "adapter": {
          "type": "TwoBitAdapter",
          "uri": "https://hgdownload.soe.ucsc.edu/goldenPath/hgFixed/bigZips/hgFixed.2bit",
          "chromSizes": "https://hgdownload.soe.ucsc.edu/goldenPath/hgFixed/bigZips/hgFixed.chrom.sizes"
        }
      }
    }
  ],
  "tracks": [
    {
      "type": "FeatureTrack",
      "trackId": "hgFixed-*",
      "name": "*",
      "assemblyNames": ["hgFixed"],
      "adapter": {
        "type": "Gff3TabixAdapter",
        "gffGzLocation": {
          "uri": "*.gff.gz"
        },
        "index": {
          "indexType": "CSI",
          "location": {
            "uri": "*.gff.gz.csi"
          }
        }
      }
    }
  ],
  "configuration": {
    "hierarchical": {
      "sort": {
        "trackNames": true,
        "categories": true
      },
      "defaultCollapsed": {
        "topLevelCategories": true,
        "subCategories": true
      }
    }
  }
}
